#include "main.h"

/**
 *print_line- imprime una linea
 *@n: variable con la que prueba
 *Return: una linea
 *
 */


void print_diagonal(int n)
{
int i;

        if (n <= 0)
        {

        }
                else
                {
                for (i = 0; i < n; i++)
                {
                        _putchar ('\\');
			_putchar ('\n');
		}       }
        _putchar ('\n');
}
